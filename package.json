    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
